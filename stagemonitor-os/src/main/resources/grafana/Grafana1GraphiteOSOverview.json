{
	"title": "OS Overview",
	"services": {
		"filter": {
			"list": [
				{
					"type": "filter",
					"name": "application",
					"query": "stagemonitor.*",
					"refresh": true,
					"includeAll": false
				},
				{
					"type": "filter",
					"name": "instance",
					"query": "stagemonitor.[[application]].*",
					"includeAll": true,
					"options": [
						{
							"text": "All",
							"value": "{host}"
						},
						{
							"text": "host",
							"value": "host"
						}
					],
					"current": {
						"text": "All",
						"value": "{host}"
					}
				},
				{
					"type": "filter",
					"name": "host",
					"query": "stagemonitor.[[application]].[[instance]].*",
					"includeAll": true,
					"options": [
						{
							"text": "All",
							"value": "*"
						}
					],
					"current": {
						"text": "All",
						"value": "*"
					}
				}
			],
			"time": {
				"from": "now-6h",
				"to": "now"
			}
		}
	},
	"rows": [
		{
			"title": "New row",
			"height": "300px",
			"editable": true,
			"collapse": false,
			"collapsable": true,
			"panels": [
				{
					"span": 6,
					"editable": true,
					"type": "graph",
					"datasource": null,
					"renderer": "flot",
					"x-axis": true,
					"y-axis": true,
					"scale": 1,
					"y_formats": [
						"short",
						"short"
					],
					"grid": {
						"leftMax": null,
						"rightMax": null,
						"leftMin": 0,
						"rightMin": null,
						"threshold1": null,
						"threshold2": null,
						"threshold1Color": "rgba(216, 200, 27, 0.27)",
						"threshold2Color": "rgba(234, 112, 112, 0.22)"
					},
					"annotate": {
						"enable": false
					},
					"resolution": 100,
					"lines": true,
					"fill": 1,
					"linewidth": 2,
					"points": false,
					"pointradius": 1,
					"bars": false,
					"stack": true,
					"legend": {
						"show": true,
						"values": false,
						"min": false,
						"max": false,
						"current": false,
						"total": false,
						"avg": false
					},
					"percentage": true,
					"zerofill": true,
					"nullPointMode": "connected",
					"steppedLine": false,
					"tooltip": {
						"value_type": "cumulative",
						"query_as_alias": true
					},
					"targets": [
						{
							"target": "sortByMinima(groupByNode(exclude(stagemonitor.[[application]].[[instance]].[[host]].cpu_usage.*,'idle'),5,'maxSeries'))",
							"hide": false
						},
						{
							"target": "groupByNode(stagemonitor.[[application]].[[instance]].[[host]].cpu_usage.idle,5,'maxSeries')",
							"hide": false
						}
					],
					"aliasColors": {
						"sys": "#7EB26D",
						"user": "#EAB839",
						"idle": "#6ED0E0",
						"wait": "#2F575E",
						"irq": "#EF843C"
					},
					"aliasYAxis": {},
					"title": "CPU utilisation"
				},
				{
					"span": 6,
					"editable": true,
					"type": "graph",
					"datasource": null,
					"renderer": "flot",
					"x-axis": true,
					"y-axis": true,
					"scale": 1,
					"y_formats": [
						"bytes",
						"short"
					],
					"grid": {
						"leftMax": null,
						"rightMax": null,
						"leftMin": 0,
						"rightMin": null,
						"threshold1": null,
						"threshold2": null,
						"threshold1Color": "rgba(216, 200, 27, 0.27)",
						"threshold2Color": "rgba(234, 112, 112, 0.22)"
					},
					"annotate": {
						"enable": false
					},
					"resolution": 100,
					"lines": true,
					"fill": 1,
					"linewidth": 2,
					"points": false,
					"pointradius": 5,
					"bars": false,
					"stack": false,
					"legend": {
						"show": true,
						"values": false,
						"min": false,
						"max": false,
						"current": false,
						"total": false,
						"avg": false
					},
					"percentage": false,
					"zerofill": true,
					"nullPointMode": "connected",
					"steppedLine": false,
					"tooltip": {
						"value_type": "cumulative",
						"query_as_alias": true
					},
					"targets": [
						{
							"target": "alias(nonNegativeDerivative(summarize(sumSeries(stagemonitor.[[application]].[[instance]].[[host]].network_io.*.read.bytes),'1min','last'),0),'receive')"
						},
						{
							"target": "alias(nonNegativeDerivative(summarize(sumSeries(stagemonitor.[[application]].[[instance]].[[host]].network_io.*.write.bytes),'1min','last'),0),'send')"
						}
					],
					"aliasColors": {},
					"aliasYAxis": {},
					"title": "Network I/O per minute"
				}
			],
			"notice": false
		},
