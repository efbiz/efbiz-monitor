	"editable": true,
	"failover": false,
	"panel_hints": true,
	"style": "dark",
	"pulldowns": [
		{
			"type": "filtering",
			"collapse": false,
			"notice": false,
			"enable": true
		},
		{
			"type": "annotations",
			"enable": false
		}
	],
	"nav": [
		{
			"type": "timepicker",
			"collapse": false,
			"notice": false,
			"enable": true,
			"status": "Stable",
			"time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"],
			"refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
			"now": true
		}
	],
	"loader": {
		"save_gist": false,
