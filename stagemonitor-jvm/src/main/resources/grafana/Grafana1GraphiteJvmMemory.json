{
	"title": "JVM Memory",
	"services": {
		"filter": {
			"list": [
				{
					"type": "filter",
					"name": "application",
					"query": "stagemonitor.*",
					"refresh": true,
					"includeAll": false
				},
				{
					"type": "filter",
					"name": "instance",
					"query": "stagemonitor.[[application]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "host",
					"query": "stagemonitor.[[application]].[[instance]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "aggregation",
					"query": "",
					"includeAll": false,
					"options": [
						{
							"value": "sum",
							"text": "sum"
						},
						{
							"value": "averageSeries",
							"text": "avg"
						},
						{
							"value": "maxSeries",
							"text": "max"
						}
					],
					"current": {
						"value": "maxSeries",
						"text": "max"
					}
				}
			],
			"time": {
				"from": "now-6h",
				"to": "now"
			}
		}
	},
	"rows": [
		{
			"title": "jvm.memoryhost",
			"height": "250px",
			"editable": true,
			"collapse": false,
			"collapsable": false,
			"panels": [
				{
					"span": 6,
					"editable": true,
					"type": "graph",
					"loadingEditor": false,
					"x-axis": true,
					"y-axis": true,
					"scale": 1,
					"grid": {
						"threshold1": null,
						"threshold2": null,
						"threshold1Color": "rgba(216, 200, 27, 0.27)",
						"threshold2Color": "rgba(234, 112, 112, 0.22)",
						"leftMax": null,
						"rightMax": null,
						"leftMin": 0,
						"rightMin": null
					},
					"annotate": {
						"enable": false,
						"query": "*",
						"size": 20,
						"field": "_type",
						"sort": ["_score", "desc"]
					},
					"auto_int": true,
					"resolution": 100,
					"interval": "10m",
					"intervals": ["auto", "1s", "1m", "5m", "10m", "30m", "1h", "3h", "12h", "1d", "1w", "1y"],
					"lines": true,
					"fill": 1,
					"linewidth": 2,
					"points": false,
					"pointradius": 5,
					"bars": false,
					"stack": false,
					"spyable": true,
					"zoomlinks": false,
					"options": false,
					"legend": {
						"show": true,
						"values": true,
						"min": false,
						"max": false,
						"current": true,
						"total": false,
						"avg": true
					},
					"interactive": true,
					"legend_counts": true,
					"timezone": "browser",
					"percentage": false,
					"zerofill": true,
					"nullPointMode": "null",
					"steppedLine": false,
					"tooltip": {
						"value_type": "cumulative",
						"query_as_alias": true
					},
					"targets": [
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_heap.max),'max')"
						},
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_heap.committed),'committed')"
						},
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_heap.used),'used')"
						}
					],
					"aliasColors": {},
					"aliasYAxis": {},
					"title": "Heap",
					"datasource": null,
					"renderer": "flot",
					"y_formats": ["bytes", "none"],
					"error": "application is not defined"
				},
				{
					"span": 6,
					"editable": true,
					"type": "graph",
					"loadingEditor": false,
					"x-axis": true,
					"y-axis": true,
					"scale": 1,
					"grid": {
						"threshold1": null,
						"threshold2": null,
						"threshold1Color": "rgba(216, 200, 27, 0.27)",
						"threshold2Color": "rgba(234, 112, 112, 0.22)",
						"leftMax": null,
						"rightMax": null,
						"leftMin": 0,
						"rightMin": null
					},
					"annotate": {
						"enable": false,
						"query": "*",
						"size": 20,
						"field": "_type",
						"sort": ["_score", "desc"]
					},
					"auto_int": true,
					"resolution": 100,
					"interval": "10m",
					"intervals": ["auto", "1s", "1m", "5m", "10m", "30m", "1h", "3h", "12h", "1d", "1w", "1y"],
					"lines": true,
					"fill": 1,
					"linewidth": 2,
					"points": false,
					"pointradius": 5,
					"bars": false,
					"stack": false,
					"spyable": true,
					"zoomlinks": false,
					"options": false,
					"legend": {
						"show": true,
						"values": true,
						"min": false,
						"max": false,
						"current": true,
						"total": false,
						"avg": true
					},
					"interactive": true,
					"legend_counts": true,
					"timezone": "browser",
					"percentage": false,
					"zerofill": true,
					"nullPointMode": "null",
					"steppedLine": false,
					"tooltip": {
						"value_type": "cumulative",
						"query_as_alias": true
					},
					"targets": [
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_non_heap.max),'max')"
						},
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_non_heap.committed),'committed')"
						},
						{
							"target": "alias([[aggregation]](stagemonitor.[[application]].[[instance]].[[host]].jvm_memory_non_heap.used),'used')"
						}
					],
					"aliasColors": {},
					"aliasYAxis": {},
					"title": "Non-Heap",
					"datasource": null,
					"renderer": "flot",
					"y_formats": ["bytes", "none"],
					"error": "application is not defined"
				}
			],
			"notice": false
		},
		{
